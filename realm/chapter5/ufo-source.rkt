#lang racket

(require 2htdp/universe 2htdp/image)

(define WIDTH 200)
(define HEIGHT 300)

(define (add-3-to-state current-state)
  (+ current-state 3))

(define (draw-a-ufo-onto-an-empty-scene current-state)
  (place-image IMAGE-of-UFO (/ WIDTH 2) current-state
               (empty-scene WIDTH HEIGHT)))

(define (state-is-300 current-state)
  (>= current-state 300))

(define (main)
  (big-bang 0                                        ;; initial state 
            (on-tick add-3-to-state)                 ;; when the clock ticks, add ...
            (to-draw draw-a-ufo-onto-an-empty-scene) ;; when the state changes, draw ... 
            (stop-when state-is-300)))               ;; when the UFO's y coordinate is 300, stop. 

;; to run the program, click run and enter 
;;   > (main) 
;; at the prompt. 

